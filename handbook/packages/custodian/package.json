{
  "name": "custodian",
  "type": "module",
  "version": "1.0.0",
  "description": "Custodian for UDR Handbook that takes care of in-app data maintenance",
  "author": "chenlm",
  "scripts": {
    "dev": "rollup -c -w",
    "serve": "npm run build && jasmine-browser-runner serve --config=jasmine.config.js",
    "test": "npm run build && jasmine-browser-runner runSpecs --config=jasmine.config.js",
    "build": "rimraf dist && rollup -c",
    "typecheck": "tsc -p tsconfig.check.json --noEmit",
    "typecheck:why": "tsc -p tsconfig.check.json --noEmit --explainFiles > explainTypes.txt"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "crypto-es": "^2.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0"
  }
}
